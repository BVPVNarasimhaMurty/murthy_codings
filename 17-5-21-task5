#include<bits/stdc++.h>
using namespace std;
int main()
{
	int n;
	cin >> n;
	while(n--)
	{
		string s;
		cin >> s;
		set<char>str;
		for(int x=0;s[x]!='\0';x++)
		{
			str.insert(s[x]);
		}
		long long int len = str.size();
		long long int res = len*(len+1)/2;
		cout << res << endl;
	}
 return 0;
}
